<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Alte Kamereren - @ViewData["Title"]</title>
    <meta property="og:title" content="Alte Kamereren - @ViewData["Title"]" />
    @if (ViewData["Description"] != null)
    {
        <meta property="og:description" content="@ViewData["Description"]" />
        <meta name="description" content="@ViewData["Description"]">
    }
    <meta property="og:image" content="https://www.altekamereren.org/images/OG.png" />
     <environment exclude="Development">
        <link rel="modulepreload" href="/dist/@assetsModel.JsVendor">
        <link rel="stylesheet" href="~/dist/@assetsModel.CssMain">
        <link rel="stylesheet" href="~/dist/@assetsModel.CssVendor">
    </environment>
    <link rel="icon" type="image/x-icon" href="/favicon.ico">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="192x192" href="/android-chrome-192x192.png">
    <link rel="icon" type="image/png" sizes="512x512" href="/android-chrome-192x192.png">

    @if (ViewData["Canonical"] != null)
    {
        <link rel="canonical" href="@ViewData["Canonical"]" />
    }
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    @RenderSection("styles", required: false)
    @await Component.InvokeAsync("StructuredData")
</head>
<body>
    @await Component.InvokeAsync("Header")
    <div class="container body-content">
        <div class="content-wrapper">
            <span class="mobile-language-change visible-xs">
                <a id="language-change-mob" class="@(t.IsEnglish() ? "SV" : "EN")" href="#">
                    @(t.IsEnglish() ? "SV" : "EN")
                </a>
            </span>
            @RenderBody()
        </div>
    </div>
    <div id="loginapp"></div>
    <environment exclude="Development">
        <script src="~/dist/@assetsModel.JsMain" type="module">
        </script>
    </environment>
    <environment include="Development">
        <script src="~/Scripts/main.ts" type="module" asp-append-version="true">
        </script>
    </environment>


    @RenderSection("scripts", required: false)
</body>
</html>