        <script src="@settings.CDN/dist/@assetsModel.JsAdmin" type="module">
        </script>
    </environment>
    <environment include="Development">
        <script src="~/Scripts/admin.ts" type="module" asp-append-version="true">
        </script>
    </environment>
}