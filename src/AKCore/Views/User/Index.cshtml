</div>

@section styles {
    <environment exclude="Development">
        <link rel="stylesheet" href="@settings.CDN/dist/@assetsModel.CssAdmin">
    </environment>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
}

@section scripts {
